<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Qi.Attendance" assembly="Qi.Attendance"> 
  <class name="CheckHistory" table="Attendance_CheckHistory">
    <id name="Id">
      <generator class="uuid.hex"></generator>
    </id>
    
    <property name="SignTime" length="30" type="String"></property>
    <property name="CardNo" length="19" type="String"></property>
    <property name="Type" type="Qi.Attendance.CheckType,Qi.Attendance"></property>
   
  </class>
</hibernate-mapping>