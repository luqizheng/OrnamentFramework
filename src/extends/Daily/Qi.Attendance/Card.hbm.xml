<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Qi.Attendance" assembly="Qi.Attendance"> 
  <class name="Card" table="Attendance_CheckHistory">
    <id name="Id">
      <generator class="uuid.hex"></generator>
    </id>
    
    
    <property name="Number" length="19" type="String"></property>
    <property name="State" type="Qi.Attendance.CardState,Qi.Attendance"></property>
    <many-to-one name="Employee" class="Employee"></many-to-one>
  </class>
</hibernate-mapping>