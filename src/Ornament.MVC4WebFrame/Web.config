        <add name="OrnamentRoleProvider" type="Ornament.Web.MemberShips.RoleProvider,Ornament.Web.Core" />
      </providers>
    </roleManager>
    <!-- customer error page-->
    <customErrors mode="Off" defaultRedirect="Error">
