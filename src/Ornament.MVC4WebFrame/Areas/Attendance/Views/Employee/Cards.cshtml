@using Resources
@model Ornament.MVCWebFrame.Areas.Attendance.Models.CardsModel
@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_appLayout.cshtml";
    
}
@Scripts.Render("~/bundles/ko")
<div class="box">
    <div class="content">
        <form method="POST">
        @Html.HiddenFor(m => m.Employee.Id)
        @Html.Partial("EditorTemplates/CardsEditor", this.Model)
        <div class="form-actions">
            <input type="submit" value="@Basic.Save" class="btn btn-primary"/>
        </div>
        </form>
    </div>
</div>
