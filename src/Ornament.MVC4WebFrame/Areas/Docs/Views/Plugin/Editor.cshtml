@using Ornament.MVCWebFrame.Models
@model dynamic
@{
    ViewBag.Title = "title";
    Layout = "../Shared/_codeLayout.cshtml";
}

<div class="page-header">
    <h2>
        Editor</h2>
</div>
<div class="box">
    <div class="content">
        <form class="form-horizontal">
        <div class="control-group">
            <label class="control-label">
                Rich Editor</label>
            <div class="controls">
                @Html.Partial("_editor", new Editor()
                {
                    EditorName = "submit_name",
                    Content = @"<h1>wysihtml5
</h1>
<p>
please use Ornament.MVCWebFrame.Models.Editor to setting. 
</p>
"
                })
            </div>
        </div>
        <div class="form-actions">
            <input value="submit" type="submit" />
        </div>
        </form>
        <pre class="prettyprint lang-vb linenums">
        @@Html.Partial("_editor", new Editor()
        {
            EditorName = "submit_name",
            Content="&lt;H1&gt; title&lt;/H1&gt;"
        };
        </pre>
    </div>
</div>
