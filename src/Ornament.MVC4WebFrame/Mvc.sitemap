    <mvcSiteMapNode resourceKey="personal" action="Index" controller="Account" imageUrl="fam-user" title="Personal Center" disabled="disabled">
    </mvcSiteMapNode>



    <mvcSiteMapNode resourceKey="membership" action="Index" controller="User" title="MemberShips" area="MemberShips" imageUrl="icon-group" clickable="false">
      <mvcSiteMapNode  area="MemberShips" action="Index" controller="User" resourceKey="userList" permission="User:Read">
        <mvcSiteMapNode area="MemberShips" action="Create" controller="User" resourceKey="userCreate" permission="User:Modify"></mvcSiteMapNode>
        <mvcSiteMapNode area="MemberShips" action="Edit" controller="User" resourceKey="userEdit" permission="User:Modify"></mvcSiteMapNode>
        <mvcSiteMapNode area="MemberShips" action="Assign" controller="User" resourceKey="assign" permission="User:Modify"></mvcSiteMapNode>
      </mvcSiteMapNode>
      <mvcSiteMapNode  area="MemberShips" controller="Role" resourceKey="roleList" permission="Role:Read">
        <mvcSiteMapNode area="MemberShips" controller="Role" action="Edit" resourceKey="roleEdit" permission="Role:Modify"></mvcSiteMapNode>
        <mvcSiteMapNode area="MemberShips" controller="Role" action="Create" resourceKey="roleEdit" permission="Role:Modify"></mvcSiteMapNode>
        <mvcSiteMapNode area="MemberShips" controller="Role" action="Delete" resourceKey="roleDelete" permission="Role:Delete"></mvcSiteMapNode>
      </mvcSiteMapNode>
      <mvcSiteMapNode area="MemberShips" controller="UserGroup" action="Index" resourceKey="groupList" permission="UserGroup:Read">
        <mvcSiteMapNode area="MemberShips" controller="UserGroup" action="Edit" resourceKey="groupEdit" permission="UserGroup:Modify"></mvcSiteMapNode>
        <mvcSiteMapNode area="MemberShips" controller="UserGroup" action="Create" resourceKey="groupCreate" permission="UserGroup:Modify"></mvcSiteMapNode>
        <mvcSiteMapNode area="MemberShips" controller="UserGroup" action="AssignUser" resourceKey="groupAssignUser" permission="UserGroup:Assign"></mvcSiteMapNode>
        <mvcSiteMapNode area="MemberShips" controller="UserGroup" action="AssignRole" resourceKey="groupAssignRole" permission="UserGroup:Assign"></mvcSiteMapNode>
      </mvcSiteMapNode>
      <mvcSiteMapNode area="MemberShips" controller="Org" action="Index" resourceKey="orgList" permission="Org:Read">
        <mvcSiteMapNode area="MemberShips" controller="Org" action="Edit" resourceKey="orgEdit" permission="Org:Modify"></mvcSiteMapNode>
        <mvcSiteMapNode area="MemberShips" controller="Org" action="Create" resourceKey="orgCreate" permission="Org:Modify"></mvcSiteMapNode>
      </mvcSiteMapNode>
      <mvcSiteMapNode area="MemberShips" controller="Permissions" resourceKey="permissionList" >
        <mvcSiteMapNode area="MemberShips" controller="Permissions" action="Edit" resourceKey="permissionEdit"></mvcSiteMapNode>
        <mvcSiteMapNode area="MemberShips" controller="Permissions" action="Create" resourceKey="permissionCreate"></mvcSiteMapNode>
      </mvcSiteMapNode>
    </mvcSiteMapNode>

    <mvcSiteMapNode title="News Manager" controller="News" area="Messages" action="Index" imageUrl="icon-comments" clickable="false">
      <mvcSiteMapNode controller="NewsType" area="Messages" action="Index" title="News Type">
        <mvcSiteMapNode controller="NewsType" area="Messages" action="Edit" title="Edit"/>
        <mvcSiteMapNode controller="NewsType" area="Messages" action="Create" title="Create"/>
      </mvcSiteMapNode>
      <mvcSiteMapNode controller="News" area="Messages" action="Index" title="News" >
        <mvcSiteMapNode controller="News" area="Messages" action="Edit" title="Edit">
        </mvcSiteMapNode>
        <mvcSiteMapNode controller="News" area="Messages" action="Create" title="Create">
        </mvcSiteMapNode>
      </mvcSiteMapNode>
    </mvcSiteMapNode>
    
    <mvcSiteMapNode resourceKey="system"  imageUrl="icon-cogs" clickable="false">
      <mvcSiteMapNode controller="NotifyType" area="Messages" action="Index" title="NotifyType Setting"></mvcSiteMapNode>
      <mvcSiteMapNode controller="Notify" area="Messages" action="Index" title="NotifyTypes"></mvcSiteMapNode>
      
      <mvcSiteMapNode controller="Site" area="Settings" action="Index" title="Information"></mvcSiteMapNode>
      <mvcSiteMapNode controller="FileManager" area="Settings" action="Index" title="File Managers"></mvcSiteMapNode>
    </mvcSiteMapNode>

    <mvcSiteMapNode title="Docs" imageUrl="icon-indent-right">
      <mvcSiteMapNode controller="TypeLayout" area="Docs" action="BaseType" title="BaseType"></mvcSiteMapNode>
      <mvcSiteMapNode controller="TypeLayout" area="Docs" action="ExtenderType" title="Extend Type"></mvcSiteMapNode>
    </mvcSiteMapNode>

  </mvcSiteMapNode>


