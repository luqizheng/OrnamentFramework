@using System.Web.Optimization

require.config({
        baseUrl:"/scripts/",
paths: {
        jqueryui:"/scripts/libs/jquery-ui-1.11.2",
        bootstrap:"@Scripts.Url("~/scripts/bootstrap.js")",
        json2: "@Scripts.Url("~/scripts/json2")",
        form:"@Scripts.Url("~/scripts/plugin/jquery-form/jquery-form.min")",
        bootbox:"@Scripts.Url("~/scripts/plugin/bootbox/bootbox.min")",
        nestable:"@Scripts.Url("~/scripts/plugin/jquery-nestable/jquery.nestable.min")",
        sparkline:"@Scripts.Url("/scripts/plugin/sparkline/jquery.sparkline")",
        vaform:"plugin/vaForm/vaForm",
        wizard:"plugin/bootstrap-wizard/jquery.bootstrap.wizard.min",
        socketio:"/scripts/libs/socket.io-1.3.2",
        ckeditor:"/scripts/plugin/ckeditor/ckeditor",
        maxlength:"/scripts/plugin/maxlength/bootstrap-maxlength.min",
        validate:'plugin/jquery-validate/jquery.validate',
        unobtrusive:'plugin/jquery.validate.unobtrusive.ajax/jquery.unobtrusive-ajax',
        bsvalid:'plugin/jquery.validate.unobtrusive.bootstrap/jquery.validate.unobtrusive.bootstrap', @*replace scripts\plugin\jquery.validate.unobtrusive\jquery.validate.unobtrusive.js*@
floatChart:'/scripts/float',
        smarkchat:'@Scripts.Url("~/scripts/smark.chat.js")',
        touch:"plugin/jquery-touch/jquery.ui.touch-punch.min",
        "app.config":"app.config",
        util:"util",
        app:"app",
        jarvis:"smartwidgets/jarvis.widget.min",
        jarvismenu:"plugin/jarvismenu/jarvismenu",
        smarkNotify:"/scripts/notification/SmartNotification.min",
        select2:"plugin/select2/select2",
        maskedinput:"plugin/masked-input/jquery.maskedinput.min",
        mbBrowser:"plugin/msie-fix/jquery.mb.browser.min",
        moment:"plugin/moment/moment.min",
        bsslider:"plugin/bootstrap-slider/bootstrap-slider.min",
        @*avalon plugin*@
        pager:"@Scripts.Url("~/scripts/avalons/pager/pager")",
        chat:"@Scripts.Url("~/scripts/avalons/chat/chat")",
        main:"@Scripts.Url("~/scripts/modules/main")",
        ornamentUi:"@Scripts.Url("~/scripts/modules/ornamentUi")",
        orgSelect:"/MemberShips/scripts/Org/Org"
},
shim:{
        
        validate:["jquery"],
        maxlength:["jquery"],
        unobtrusive:["jquery"],
        bsvalid:['jquery','validate','unobtrusive'],
        vaform:['jquery','validate','unobtrusive','bsvalid'],
        bootstrap:['jquery'],
        jarvismenu:["jquery"],
        touch:['jquery'],
        "app.config":['jquery'],
        util:['bootstrap','app'],
        app:['jquery',"jarvis","jarvismenu","sparkline",@*"floatChart"*@],
        jarvis:['jquery'],
        smarkchat:['jqueryui'],
        touch:['jqueryui'],
        smarkNotify:["jquery"],
        select2:["jquery"],
        maskedinput:["jquery"],
        mbBrowser:["jquery"],
        bsslider:["jquery"],
        bootbox:["bootstrap"]
    }
});




