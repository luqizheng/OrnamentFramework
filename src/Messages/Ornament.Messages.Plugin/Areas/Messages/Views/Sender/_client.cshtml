@model Ornament.Messages.Notification.Senders.ClientSender
<div class="form-group">
    @Html.LabelFor(s => s.Server)
    @Html.TextBoxFor(s => s.Server, new Dictionary<string, object>()
    {
        {"ms-duplex","Client.Server"}
    })
    @Html.ValidationMessageFor(s => s.Server)
</div>

<div class="form-group">
    @Html.LabelFor(s => s.PrivateCode)
    @Html.TextBoxFor(s => s.Server, new Dictionary<string, object>()
        {
            {"ms-duplex","Client.PrivateCode"}
        })
    @Html.ValidationMessageFor(s => s.PrivateCode)
</div>


