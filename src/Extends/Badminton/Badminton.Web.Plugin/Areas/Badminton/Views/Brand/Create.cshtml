@using Ornament.Properties
@using Ornament
@model Badminton.Consumableses.Brand

@{
    ViewBag.Title = "Create";
    Layout = OrnamentContext.Configuration.Layout();
}
@section pageTitle{
    <h5>Creat3e Brand</h5>
}
@using (Html.BeginForm("Create", "Brand", FormMethod.Post))
{
    <div class="widget">
        @*<div class="navbar">
                <div class="navbar-inner">
                </div>
            </div>*@

        <div class="well form-horizontal">
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true)
            @Html.EditorFor(model => model.Name)
        </div>

        <div class="form-actions">
            <button class="btn btn-primary">@Resources.Create</button>
            <a href="@Url.Action("Index")" class="btn">@Resources.Cancel</a>
        </div>
    </div>

}


