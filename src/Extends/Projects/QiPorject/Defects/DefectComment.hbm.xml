<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="QiProject.Defects" assembly="QiProject">
  <class name="DefectComment" table="Prj_DefectComment">
    <id name="Id" length="32">
      <generator class="uuid.hex"></generator>
    </id>
    <property name="Description" length="1024"></property>
    <property name="CreateTime" access="nosetter.camelcase-underscore"></property>
    <many-to-one class="Ornament.MemberShip.User" name="Creater"></many-to-one>
  </class>
</hibernate-mapping>